{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "dev",
			"program": "${workspaceFolder}/node_modules/.bin/webpack-dev-server"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "source",
			"program": "${workspaceRoot}/node/index.js",
			"args": [
				"-dev"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "bin",
			"sourceMaps": true,
			"program": "${workspaceRoot}/bin/rex-node.js",
			"args": [
				"./bin/test/a.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "read me",
			"program": "${workspaceFolder}/bin/rex-node.js",
			"args": [
				"./node/read-me.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "uglify",
			"program": "${workspaceRoot}/node/uglify.js"
		}
	]
}